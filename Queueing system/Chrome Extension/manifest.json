{
    "name": "QSS",
    "version": "1.0",
    "description": "Queueing System Styling",
    "permissions": [
      "tabs", "<all_urls>"
    ],
    "browser_action":{
      "default_popup": "popup.html",
      "default_title": "Oooooooh Weeee"
    },
    "content_scripts": [
    {
     "matches": ["http://*/*", "https://*/*"],
     "js":["stylingscript.js"],
     "run_at": "document_end"
    }
    ],
    "manifest_version": 2
  }